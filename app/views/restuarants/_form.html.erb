<div class ="content">
  <%= form_for @restuarant do |f| %>
    <br>
    <%= f.label :name %>
    <%= f.text_field :name %><br><br>

    <%= f.label :description %>
    <%= f.text_area :description %><br>

    <%= f.label :location %>
    <%= f.text_field :location %><br>

    <%= f.label :rating %>
    <%= f.number_field :rating , :step => 0.01%><br>

    <%= f.label :likes %>
    <%= f.number_field :likes %><br><br>

    <%= f.submit %>
  <% end %>
</div>